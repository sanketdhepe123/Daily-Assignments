// Java program to accept a number from user and calculate its square if it is divisble by 5
import java.util.*;

class first {
    public static void main(String[] args) {
    
    //asking user for input a number
    
        System.out.println("enter a number ");
        //scanning a number 
        
        Scanner sc = new Scanner(System.in);
        int num1=sc.nextInt();
        
        //calculating a square if enterd number is divisibe 5
        
        if(num1%5==0)
        {
            int sqr= num1*num1;
            System.out.println("entered number square is  "+sqr);
        }
        else 
        {
            System.out.println("Number is not divisible by 5");
        }
        
    }
}
